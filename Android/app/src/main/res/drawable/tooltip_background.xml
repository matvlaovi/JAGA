<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="2dp" android:height="2dp"
                android:color="@color/colorDarkBackground" />
            <solid android:color="#00000000" />
            <padding android:left="4dp" android:top="4dp" android:right="4dp"
            android:bottom="4dp" />

            <corners android:radius="5dp" android:bottomRightRadius="5dp"
                android:bottomLeftRadius="5dp" android:topLeftRadius="5dp"
                android:topRightRadius="5dp" />
        </shape>
    </item>
</layer-list>